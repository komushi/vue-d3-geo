{
  "name": "vue-d3-geo",
  "version": "0.2.1",
  "description": "A D3 Geo Vue.js Library",
  "author": "Lei Xu <komushi@gmail.com>",
  "homepage": "https://github.com/komushi/vue-d3-geo/",
  "license": "ISC",
  "main": "dist/vue-d3-geo.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:demo",
    "lint": "vue-cli-service lint",
    "build:demo": "vue-cli-service build --dest demo/",
    "build:lib": "vue-cli-service build --target lib --name vue-d3-geo ./src/lib.js --dest dist/"
  },
  "dependencies": {
    "d3": "^5.12.0",
    "leaflet": "^1.5.1",
    "leaflet-providers": "^1.9.0",
    "topojson": "^3.0.2",
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "core-js": "^2.6.5",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "files": [
    "dist"
  ]
}
